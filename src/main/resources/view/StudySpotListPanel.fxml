<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Label?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <HBox alignment="CENTER_LEFT" spacing="48">
    <Label text="StudyTracker" styleClass="h1-size"/>
    <Label fx:id="studySpotListCount" styleClass="icon_container" text="0"/>
  </HBox>
  <StackPane>
    <ListView fx:id="studySpotListView" VBox.vgrow="ALWAYS" />
    <Label fx:id="studySpotListViewDefaultMessage" styleClass="italics"/>
  </StackPane>
</VBox>
